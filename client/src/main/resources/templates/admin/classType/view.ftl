<div id="classType-view-modal" class="modal fade" tabindex="-1">
    <form id="classType-view-form" action="" method="get">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="blue bigger">分类</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>

                <div class="modal-body overflow-visible">
                    <div class="row">

                        <div class="col-xs-12 col-sm-12">

                            <table class="table table-bordered table-striped m-b-0"><!-- table-hover-->
                                <tbody>
                                <tr>
                                    <th class="width-80">id</th>

                                    <td>
                                        <div class="overflow-auto" id="classType-view-id"></div>
                                    </td>
                                </tr>
                                <tr>
                                    <th class="width-80">名称</th>

                                    <td>
                                        <div class="overflow-auto" id="classType-view-name"></div>
                                    </td>
                                </tr>
                                <tr>
                                    <th class="width-80">编码</th>

                                    <td>
                                        <div class="overflow-auto" id="classType-view-code"></div>
                                    </td>
                                </tr>
                                <tr>
                                    <th class="width-80">排序</th>

                                    <td>
                                        <div class="overflow-auto" id="classType-view-soft"></div>
                                    </td>
                                </tr>
                                <tr>
                                    <th class="width-80">父类编号</th>

                                    <td>
                                        <div class="overflow-auto" id="classType-view-parentCode"></div>
                                    </td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

                <div class="modal-footer">
                    <button class="btn btn-lime" data-dismiss="modal">
                        关闭
                    </button>
                </div>
            </div>
        </div>
    </form>
</div>